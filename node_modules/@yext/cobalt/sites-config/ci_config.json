{
  "artifactStructure": {
      "assets": [
          {
              "root": "storybook-static"
          }
      ],
      "hbsTemplates": [
          { 
              "root": "templates",
              "pattern": "*.hbs"
          },
          { 
              "root": "partials",
              "pattern": "*.hbs"
          }
      ]
  },
  "base_image_tag": "yextci/yextci-builder:stable",
  "dependencies": {
      "installDepsCmd": "echo '//registry.npmjs.org/:_authToken=${NPM_TOKEN}' > .npmrc; npm install",
      "requiredFiles": ["package.json"]
  },
  "buildArtifacts": {
      "build_setup_cmd": "",
      "build_cmd": "npm run build-storybook"
  },
  "livePreview": {
      "serve_setup_cmd": "",
      "serve_cmd": ""
  },
  "watch_cmd": ""
}